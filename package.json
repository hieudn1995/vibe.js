{
  "name": "discord-music-bot",
  "version": "1.1.2",
  "description": "A simple music bot for Discord",
  "homepage": "",
  "files": [
    "lib"
  ],
  "main": "dist/index.js",
  "keywords": [
    ""
  ],
  "devDependencies": {
    "clapp": "^1.3.0",
    "discord.js": "^11.2.1",
    "eslint": "^4.1.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-xo": "^0.18.2",
    "eslint-plugin-prettier": "^2.2.0",
    "husky": "^0.14.3",
    "jest": "^21.0.0",
    "jest-cli": "^21.0.0",
    "lint-staged": "^4.2.1",
    "node-opus": "^0.2.4",
    "nsp": "^2.6.3",
    "prettier": "^1.7.0",
    "request": "^2.79.0",
    "ytdl-core": "^0.7.24"
  },
  "scripts": {
    "start": "node ./lib/index.js"
  },
  "dependencies": {
    "node-gyp": "^3.6.2",
    "valid-url": "^1.0.9",
    "youtube-search": "^1.0.10"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "https://github.com/tannerz28/discord-music-bot.git",
  "jest": {
    "testEnvironment": "node"
  },
  "license": "GPL-3.0"
}
